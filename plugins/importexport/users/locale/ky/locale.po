msgid "plugins.importexport.users.displayName"
msgstr "Колдонуучулардын XML плагини"

msgid "plugins.importexport.users.description"
msgstr "Колдонуучуларды импорттоо жана экспорттоо"

msgid "plugins.importexport.users.cliUsage"
msgstr ""
"Колдонуу: {$scriptName} {$pluginName} [буйрук] ...\n"
"Буйруктар:\n"
" импорттоо [xmlFileName] [journal_path]\n"
" экспорттоо [xmlFileName] [journal_path]\n"
" экспорттоо [xmlFileName] [journal_path] [userId1] [userId2] ...\n"

msgid "plugins.importexport.users.cliUsage.examples"
msgstr ""
"\n"
"Мисалдар:\n"
" myImportFile.xml сайтынан myJournalга колдонуучуларды импорттоо:\n"
" {$scriptName} {$pluginName} myImportFile.xml myJournal импорттоо\n"
"\n"
" myJournal'дан бардык колдонуучуларды экспорттоо:\n"
" {$scriptName} {$pluginName} myExportFile.xml myJournal экспорттоо\n"
"\n"
" Идентификатору менен көрсөтүлгөн колдонуучуларды экспорттоо:\n"
" {$scriptName} {$pluginName} myExportFile.xml myJournal 1 2 экспорттоо\n"
